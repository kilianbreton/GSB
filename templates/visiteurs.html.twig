{% extends 'base.html.twig' %}
{% block body %}
        <table>
        <tr>
            <th>Mat</th>
            <th>Login</th>
            <th>Nom</th>
            <th>Prenom</th>
            <th>Adresse</th>
            <th>CP</th>
            <th>Ville</th>
            <th>Date d'embauche</th>
            <th>Labo</th>
            <th>Secteur</th>
        </tr>
        {% for vis in visiteurs %}
            <tr>
                <th>{{ vis.getMat }}</th>
                <th>{{ vis.getLogin }}</th>
                <th>{{ vis.getNom }}</th>
                <th>{{ vis.getPrenom }}</th>
                <th>{{ vis.getAdresse }}</th>
                <th>{{ vis.getCp }}</th>
                <th>{{ vis.getVille }}</th>
                <th>{{ vis.getDateEmbauche }}</th>
                <th>{{ vis.getLabCode }}</th>
                <th>{{ vis.getSecCode }}</th>
                
            </tr> 
        {% endfor %}
{% endblock %}